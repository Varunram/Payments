<% @links.each do |link| %>
 <% if user_signed_in? %>
  <% if link.auth == current_user.auth %>
    <div class="link row clearfix">
      <h2>
        <%= link_to link.title, link %><br>
        <small class="author">Submitted <%= time_ago_in_words(link.created_at) %> by <%= link.user.name %></small>
      </h2>

      <div class="btn-group">
        <a class="btn btn-default" href="<%= link.url %>">Visit Link</a>
      </div>
    </div>

  <%else%>
    <h3> You have Not won any bids Till Now </h3>
  <%end%>
 <%end%>
<% end %>
